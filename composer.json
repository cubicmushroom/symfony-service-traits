{
    "name": "cubicmushroom/symfony-service-traits",
    "version": "0.0.0",
    "description": "Collection of traits to make injecting standard Symfony services into classes easy",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Toby Griffiths",
            "email": "toby@cubicmushroom.co.uk"
        }
    ],
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require": {
        "swiftmailer/swiftmailer": ">=4.2.0,~5.0",
        "symfony/translation": "~2.7",
        "symfony/security-core": "~2.6.13|~2.7.9|~2.8",
        "symfony/twig-bundle": "~1.23|~2.0",
        "symfony/routing": "~2.6,>2.6.4"
    },
    "require-dev": {
        "phpspec/phpspec": "^2.5"
    },
    "autoload": {
        "psr-4": {
            "CubicMushroom\\Symfony\\ServiceTrait\\": "src/ServiceTrait"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-develop": "2.7-dev"
        }
    }
}
